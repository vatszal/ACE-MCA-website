{
  "env": {
    "browser": true,
    "es2022": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["plugin:react/recommended", "standard-with-typescript"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks"],
  "rules": {
    "linebreak-style": 0,
    "react/prop-types": "off",
    "multiline-ternary": "off",
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "indent": "off",
    "no-path-concat": "off"
  }
}
